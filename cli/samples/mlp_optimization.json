{
  "hp_space": {
    "model": {
      "class_name": "MLP",
      "config": {
        "name": "mlp_opt",
        "layers": {
          "node_type": "hp_list",
          "value": [
            {
              "node_type": "hp_int",
              "min_val": 10,
              "max_val": 100
            },
            {
              "node_type": "hp_int",
              "min_val": 10,
              "max_val": 100
            }
          ]
        },
        "dropout": {
          "node_type": "hp_float",
          "min_val": 0,
          "max_val": 0.3
        }
      }
    }
  },

  "opt": {
    "method": "cmaes",
    "params": {
      "pop_size": 5,
      "max_iter": 5
    },

    "obj_fn": {
      "type": "cv",
      "method": "split",
      "params": {
        "test_size": 0.3
      },
      "scoring": "mse"
    }
  },

  "data_set": {
    "data_x": {
      "path": "/path/to/your/train/x/csv/with/header",
      "params": {
        "has_header": true
      }
    },
    "data_y": {
      "path": "/path/to/your/train/y/csv/with/header",
      "params": {
        "has_header": true
      }
    }
  }
}